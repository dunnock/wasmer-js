{
  "name": "@wasmer/wasm-terminal",
  "version": "0.2.3",
  "description": "A terminal-like component for the browser, that fetches and runs Wasm modules in the context of a shell. üêö",
  "module": "dist/unoptimized/wasm-terminal.esm.js",
  "browser": "dist/unoptimized/wasm-terminal.iife.js",
  "typings": "dist/packages/wasm-terminal/lib/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "../../LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:prod",
    "build:prod": "npx rollup -c --environment LIB,WORKER,PROD",
    "build:dev": "npx rollup -c --environment LIB,WORKER,DEV",
    "dev": "npx watch \"npm run build:dev\" lib",
    "test": "npx jest --config jest.config.js",
    "test:watch": "npx jest --config jest.config.js --watchAll",
    "docs": "npx typedoc lib/ --out docs --target es6 --theme minimal --mode file"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wasmerio/wasmer-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "wasm",
    "wasi",
    "tty",
    "shell",
    "terminal",
    "html",
    "xterm",
    "dom",
    "browser"
  ],
  "author": "Aaron Turner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wasmerio/wasmer-js/issues"
  },
  "homepage": "https://github.com/wasmerio/wasmer-js/tree/master/packages/wasm-terminal",
  "dependencies": {
    "@wasmer/wasi": "file:../wasi",
    "@wasmer/wasm-transformer": "file:../wasm-transformer",
    "@wasmer/wasmfs": "file:../wasmfs",
    "comlink": "^4.0.2",
    "shell-parse": "0.0.2",
    "shell-quote": "^1.7.1",
    "xterm": "^4.1.0",
    "xterm-addon-fit": "^0.2.1",
    "xterm-addon-web-links": "^0.2.1",
    "xterm-addon-webgl": "^0.2.1"
  }
}
